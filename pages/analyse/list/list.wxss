/* pages/analyse/list/list.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #EEEEEE;
}
.page{
  margin-top: 40rpx;
}
.navigator {
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  align-items: center;
  background-color: white;
}
.navigator:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0; 
  right: 30rpx;
  height: 1px;
  border-top: 1rpx solid #D8D8D8;
  color: #D8D8D8;
}
.navigator:first-child:before {
  display: none;
}
.navigator-text {
  flex: 1;
  margin-left: 20rpx;
}
.navigator-arrow {
  padding-right: 26rpx;
  position: relative;
}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}
image{
  width: 60rpx;
  height: 60rpx;
}